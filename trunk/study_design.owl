     xml:base="http://purl.org/net/OCRe/study_design.owl"
     xmlns:Outcome_before_main_predictor="&study_design2;Outcome_before_main_predictor/"
     xmlns:Outcome_after_main_predictor="&study_design2;Outcome_after_main_predictor/"
