<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY swrl "http://www.w3.org/2003/11/swrl#" >
    <!ENTITY swrlb "http://www.w3.org/2003/11/swrlb#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY owl2xml "http://www.w3.org/2006/12/owl2-xml#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY study_design "http://purl.org/net/OCRe/study_design#" >
    <!ENTITY research "http://purl.org/net/OCRe/research.owl#" >
    <!ENTITY statistics "http://purl.org/net/OCRe/statistics.owl#" >
    <!ENTITY study_design2 "http://purl.org/net/OCRe/study_design.owl#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY study_protocol "http://purl.org/net/OCRe/study_protocol.owl#" >
]>


<rdf:RDF xmlns="http://purl.org/net/OCRe/study_design#"
     xml:base="http://purl.org/net/OCRe/study_design.owl"
     xmlns:owl2xml="http://www.w3.org/2006/12/owl2-xml#"
     xmlns:study_design2="http://purl.org/net/OCRe/study_design.owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:study_design="http://purl.org/net/OCRe/study_design#"
     xmlns:study_protocol="http://purl.org/net/OCRe/study_protocol.owl#"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:statistics="http://purl.org/net/OCRe/statistics.owl#"
     xmlns:research="http://purl.org/net/OCRe/research.owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
    <owl:Ontology rdf:about="">
        <rdfs:comment rdf:datatype="&xsd;string"
            >An ontology of study design descriptors and categorization of study types in terms of their design descriptors.</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:AnnotationProperty rdf:about="&statistics;curator"/>
    <owl:AnnotationProperty rdf:about="&study_protocol;develop_comment"/>
    <owl:AnnotationProperty rdf:about="&statistics;definition"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/net/OCRe/research.owl#has_study_characteristic -->

    <owl:ObjectProperty rdf:about="&research;has_study_characteristic"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/net/OCRe/research.owl#Allocation_concealment_type -->

    <owl:Class rdf:about="&research;Allocation_concealment_type">
        <rdfs:subClassOf rdf:resource="&research;Study_design_characteristic"/>
        <owl:disjointWith rdf:resource="&research;Allocation_type"/>
    </owl:Class>
    


    <!-- http://purl.org/net/OCRe/research.owl#Allocation_type -->

    <owl:Class rdf:about="&research;Allocation_type">
        <rdfs:subClassOf rdf:resource="&research;Study_design_characteristic"/>
        <owl:disjointWith rdf:resource="&research;Randomization_method"/>
    </owl:Class>
    


    <!-- http://purl.org/net/OCRe/research.owl#Analysis_of_cost_flag -->

    <owl:Class rdf:about="&research;Analysis_of_cost_flag">
        <rdfs:subClassOf rdf:resource="&research;Study_design_characteristic"/>
    </owl:Class>
    


    <!-- http://purl.org/net/OCRe/research.owl#Assignment_characteristics -->

    <owl:Class rdf:about="&research;Assignment_characteristics">
        <rdfs:subClassOf rdf:resource="&research;Study_design_characteristic"/>
        <owl:disjointWith rdf:resource="&research;Control_group_characteristics"/>
    </owl:Class>
    


    <!-- http://purl.org/net/OCRe/research.owl#Blinding_type -->

    <owl:Class rdf:about="&research;Blinding_type">
        <rdfs:subClassOf rdf:resource="&research;Study_design_characteristic"/>
    </owl:Class>
    


    <!-- http://purl.org/net/OCRe/research.owl#Block_allocation -->

    <owl:Class rdf:about="&research;Block_allocation">
        <rdfs:subClassOf rdf:resource="&research;Randomization_method"/>
        <owl:disjointWith rdf:resource="&research;Minimization"/>
    </owl:Class>
    


    <!-- http://purl.org/net/OCRe/research.owl#Case -->

    <owl:Class rdf:about="&research;Case">
        <rdfs:subClassOf rdf:resource="&research;Control_group_type"/>
    </owl:Class>
    


    <!-- http://purl.org/net/OCRe/research.owl#Case_and_control_in_same_person -->

    <owl:Class rdf:about="&research;Case_and_control_in_same_person">
        <rdfs:subClassOf rdf:resource="&research;Main_control_group_definition"/>
        <owl:disjointWith rdf:resource="&research;Case_and_control_not_in_same_person"/>
    </owl:Class>
    


    <!-- http://purl.org/net/OCRe/research.owl#Case_and_control_not_in_same_person -->

    <owl:Class rdf:about="&research;Case_and_control_not_in_same_person">
        <rdfs:subClassOf rdf:resource="&research;Main_control_group_definition"/>
    </owl:Class>
    


    <!-- http://purl.org/net/OCRe/research.owl#Case_control_study -->

    <owl:Class rdf:about="&research;Case_control_study">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&research;Observational_analytic_study"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&research;has_study_characteristic"/>
                        <owl:someValuesFrom rdf:resource="&research;External_control_group"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&research;has_study_characteristic"/>
                        <owl:someValuesFrom rdf:resource="&research;Main_control_group_defined_by_case_status"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://purl.org/net/OCRe/research.owl#Case_crossover_study -->

    <owl:Class rdf:about="&research;Case_crossover_study">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&research;Observational_analytic_study"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&research;has_study_characteristic"/>
                        <owl:someValuesFrom rdf:resource="&research;Case_and_control_in_same_person"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&research;has_study_characteristic"/>
                        <owl:someValuesFrom rdf:resource="&research;Main_control_group_defined_by_case_status"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://purl.org/net/OCRe/research.owl#Cohort_study -->

    <owl:Class rdf:about="&research;Cohort_study">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&research;Observational_analytic_study"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&research;has_study_characteristic"/>
                        <owl:someValuesFrom rdf:resource="&research;Main_control_group_defined_by_exposure_status"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&research;has_study_characteristic"/>
                        <owl:someValuesFrom rdf:resource="&research;Outcome_measured_after_measurement_of_predictors"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://purl.org/net/OCRe/research.owl#Control_group_cardinality -->

    <owl:Class rdf:about="&research;Control_group_cardinality">
        <rdfs:subClassOf rdf:resource="&research;Control_group_characteristics"/>
    </owl:Class>
    


    <!-- http://purl.org/net/OCRe/research.owl#Control_group_characteristics -->

    <owl:Class rdf:about="&research;Control_group_characteristics">
        <rdfs:subClassOf rdf:resource="&research;Study_design_characteristic"/>
    </owl:Class>
    


    <!-- http://purl.org/net/OCRe/research.owl#Control_group_locality -->

    <owl:Class rdf:about="&research;Control_group_locality">
        <rdfs:subClassOf rdf:resource="&research;Control_group_characteristics"/>
    </owl:Class>
    


    <!-- http://purl.org/net/OCRe/research.owl#Control_group_type -->

    <owl:Class rdf:about="&research;Control_group_type">
        <rdfs:subClassOf rdf:resource="&research;Control_group_characteristics"/>
    </owl:Class>
    


    <!-- http://purl.org/net/OCRe/research.owl#Cross-sectional_study -->

    <owl:Class rdf:about="&research;Cross-sectional_study">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&research;Observational_analytic_study"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&research;has_study_characteristic"/>
                        <owl:someValuesFrom rdf:resource="&research;Main_control_group_defined_by_exposure_status"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&research;has_study_characteristic"/>
                        <owl:someValuesFrom rdf:resource="&research;Outcome_measured_at_the_same_time_as_predictors"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://purl.org/net/OCRe/research.owl#Crossover_study -->

    <owl:Class rdf:about="&research;Crossover_study">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&research;Interventional_study"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&research;has_study_characteristic"/>
                        <owl:someValuesFrom rdf:resource="&research;Has_control_group"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&research;has_study_characteristic"/>
                        <owl:someValuesFrom rdf:resource="&research;Within_subject_control_locality"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&research;has_study_characteristic"/>
                        <owl:someValuesFrom>
                            <owl:Class>
                                <owl:unionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="#Data_analyzed_across_multiple_participants"/>
                                    <rdf:Description rdf:about="#Treatment_assigned_to_multiple_participants"/>
                                </owl:unionOf>
                            </owl:Class>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <study_protocol:develop_comment
            >Simona: see change in equivalent class definition. However, I don&#39;t know how to capture &quot;and/or&quot;</study_protocol:develop_comment>
        <statistics:definition
            >A type of clinical trial comparing two or more interventions in which the participants, upon completion of the course of one treatment, are switched to another. For example, for a comparison of treatments
A and B, the participants are randomly allocated to receive them in either the order A, B or the order B, A. [Glossary of Terms in The Cochrane Collaboration]</statistics:definition>
        <statistics:curator>Simona</statistics:curator>
    </owl:Class>
    


    <!-- http://purl.org/net/OCRe/research.owl#Decision_strategy -->

    <owl:Class rdf:about="&research;Decision_strategy">
        <rdfs:subClassOf rdf:resource="&research;Unit_of_analysis"/>
        <statistics:curator>Simona</statistics:curator>
        <statistics:definition
            >An option in the decision-making process.</statistics:definition>
        <study_protocol:develop_comment
            >Simona: Ask Ida to review this definition</study_protocol:develop_comment>
    </owl:Class>
    


    <!-- http://purl.org/net/OCRe/research.owl#Deterministic_allocation -->

    <owl:Class rdf:about="&research;Deterministic_allocation">
        <rdfs:subClassOf rdf:resource="&research;Allocation_type"/>
    </owl:Class>
    


    <!-- http://purl.org/net/OCRe/research.owl#Ecologic_study -->

    <owl:Class rdf:about="&research;Ecologic_study">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&research;has_study_characteristic"/>
                <owl:someValuesFrom rdf:resource="&research;Population_as_unit_of_analysis"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&research;Non-individual_human_study"/>
        <statistics:definition
            >Epidemiological study in which the unit of observation is a population or community. [Epidemiology for the Uninitiated, available at http://www.bmj.com/epidem/epid.html]</statistics:definition>
        <statistics:curator>Simona</statistics:curator>
    </owl:Class>
    


    <!-- http://purl.org/net/OCRe/research.owl#External_control_group -->

    <owl:Class rdf:about="&research;External_control_group">
        <rdfs:subClassOf rdf:resource="&research;Control_group_locality"/>
        <owl:disjointWith rdf:resource="&research;Within_subject_control_locality"/>
    </owl:Class>
    


    <!-- http://purl.org/net/OCRe/research.owl#Field_study -->

    <owl:Class rdf:about="&research;Field_study">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&research;Qualitative_human_study"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&research;has_study_characteristic"/>
                        <owl:someValuesFrom rdf:resource="&research;Observed_in_the_field"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <statistics:definition
            >Research carried out on location: a piece of research undertaken outside the laboratory or place of learning, usually in a natural environment or among the general public. [MSN Encarta Dictionary]
For ex., Participant observation, Ethnographic study, Software field testing.</statistics:definition>
        <statistics:curator>Simona</statistics:curator>
    </owl:Class>
    


    <!-- http://purl.org/net/OCRe/research.owl#Has_control_group -->

    <owl:Class rdf:about="&research;Has_control_group">
        <rdfs:subClassOf rdf:resource="&research;Control_group_cardinality"/>
        <owl:disjointWith rdf:resource="&research;No_control_group"/>
    </owl:Class>
    


    <!-- http://purl.org/net/OCRe/research.owl#Human_subject -->

    <owl:Class rdf:about="&research;Human_subject">
        <rdfs:subClassOf rdf:resource="&research;Unit_of_analysis"/>
        <statistics:definition
            >An individual about whom an investigator conducting research obtains data through intervention or interaction with individual or identifiable private information. [45 CFR &#167;46.102(f)]</statistics:definition>
        <statistics:curator>Simona</statistics:curator>
    </owl:Class>
    


    <!-- http://purl.org/net/OCRe/research.owl#Include_analysis_of_cost -->

    <owl:Class rdf:about="&research;Include_analysis_of_cost">
        <rdfs:subClassOf rdf:resource="&research;Analysis_of_cost_flag"/>
        <owl:disjointWith rdf:resource="&research;No_analysis_of_cost"/>
    </owl:Class>
    


    <!-- http://purl.org/net/OCRe/research.owl#Individual_qualitative_study -->

    <owl:Class rdf:about="&research;Individual_qualitative_study">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&research;Qualitative_human_study"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&research;has_study_characteristic"/>
                        <owl:someValuesFrom rdf:resource="&research;Not_observed_in_the_field"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&research;has_study_characteristic"/>
                        <owl:someValuesFrom rdf:resource="&research;subject_assessed_individually"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <statistics:definition
            >A study in which human subjects are assessed individually and not in the field (their environment). For ex., 1-on-1 interview, In-lab protocol analysis.</statistics:definition>
        <statistics:curator>Simona</statistics:curator>
    </owl:Class>
    


    <!-- http://purl.org/net/OCRe/research.owl#Instrument_development_study -->

    <owl:Class rdf:about="&research;Instrument_development_study">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&research;has_study_characteristic"/>
                <owl:someValuesFrom rdf:resource="&research;Measurement_instrument"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&research;Non-individual_human_study"/>
        <study_protocol:develop_comment
            >Simona: This category is not in the most recent study design categorizer</study_protocol:develop_comment>
    </owl:Class>
    


    <!-- http://purl.org/net/OCRe/research.owl#Intervention_cardinality -->

    <owl:Class rdf:about="&research;Intervention_cardinality">
        <rdfs:subClassOf rdf:resource="&research;Assignment_characteristics"/>
    </owl:Class>
    


    <!-- http://purl.org/net/OCRe/research.owl#Interventional_study -->

    <owl:Class rdf:about="&research;Interventional_study">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&research;Quantitative_human_study"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&research;has_study_characteristic"/>
                        <owl:someValuesFrom rdf:resource="&research;Investigator_assigns_intervention"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <statistics:definition
            >Synonym: clinical trial. 
A clinical trial is any research study that prospectively assigns human participants or groups of humans to one or more health-related interventions to evaluate the effects on health outcomes. Clinical trials may also be referred to as interventional trials. Interventions include but are not restricted to drugs, cells and other biological products, surgical procedures, radiologic procedures, devices, behavioural treatments, process-of-care changes, preventive care, etc. [WHO, available at: http://www.who.int/ictrp/en/]</statistics:definition>
        <statistics:curator>Simona</statistics:curator>
    </owl:Class>
    


    <!-- http://purl.org/net/OCRe/research.owl#Investigator_assignment_role -->

    <owl:Class rdf:about="&research;Investigator_assignment_role">
        <rdfs:subClassOf rdf:resource="&research;Assignment_characteristics"/>
    </owl:Class>
    


    <!-- http://purl.org/net/OCRe/research.owl#Investigator_assigns_intervention -->

    <owl:Class rdf:about="&research;Investigator_assigns_intervention">
        <rdfs:subClassOf rdf:resource="&research;Investigator_assignment_role"/>
        <owl:disjointWith rdf:resource="&research;Investigator_assigns_no_intervention"/>
    </owl:Class>
    


    <!-- http://purl.org/net/OCRe/research.owl#Investigator_assigns_no_intervention -->

    <owl:Class rdf:about="&research;Investigator_assigns_no_intervention">
        <rdfs:subClassOf rdf:resource="&research;Investigator_assignment_role"/>
    </owl:Class>
    


    <!-- http://purl.org/net/OCRe/research.owl#Investigator_blinding -->

    <owl:Class rdf:about="&research;Investigator_blinding">
        <rdfs:subClassOf rdf:resource="&research;Blinding_type"/>
    </owl:Class>
    


    <!-- http://purl.org/net/OCRe/research.owl#Location_of_observation -->

    <owl:Class rdf:about="&research;Location_of_observation">
        <rdfs:subClassOf rdf:resource="&research;Study_design_characteristic"/>
    </owl:Class>
    


    <!-- http://purl.org/net/OCRe/research.owl#Main_control_group_defined_by_case_status -->

    <owl:Class rdf:about="&research;Main_control_group_defined_by_case_status">
        <rdfs:subClassOf rdf:resource="&research;Main_control_group_definition"/>
        <owl:disjointWith rdf:resource="&research;Main_control_group_defined_by_exposure_status"/>
    </owl:Class>
    


    <!-- http://purl.org/net/OCRe/research.owl#Main_control_group_defined_by_exposure_status -->

    <owl:Class rdf:about="&research;Main_control_group_defined_by_exposure_status">
        <rdfs:subClassOf rdf:resource="&research;Main_control_group_definition"/>
    </owl:Class>
    


    <!-- http://purl.org/net/OCRe/research.owl#Main_control_group_definition -->

    <owl:Class rdf:about="&research;Main_control_group_definition">
        <rdfs:subClassOf rdf:resource="&research;Study_design_characteristic"/>
    </owl:Class>
    


    <!-- http://purl.org/net/OCRe/research.owl#Matched_cohort -->

    <owl:Class rdf:about="&research;Matched_cohort">
        <rdfs:subClassOf rdf:resource="&research;Control_group_type"/>
    </owl:Class>
    


    <!-- http://purl.org/net/OCRe/research.owl#Measurement_instrument -->

    <owl:Class rdf:about="&research;Measurement_instrument">
        <rdfs:subClassOf rdf:resource="&research;Unit_of_analysis"/>
        <study_protocol:develop_comment
            >Simona: this should be removed if not referenced</study_protocol:develop_comment>
    </owl:Class>
    


    <!-- http://purl.org/net/OCRe/research.owl#Minimization -->

    <owl:Class rdf:about="&research;Minimization">
        <rdfs:subClassOf rdf:resource="&research;Randomization_method"/>
    </owl:Class>
    


    <!-- http://purl.org/net/OCRe/research.owl#Mode_of_inquiry -->

    <owl:Class rdf:about="&research;Mode_of_inquiry">
        <rdfs:subClassOf rdf:resource="&research;Study_design_characteristic"/>
    </owl:Class>
    


    <!-- http://purl.org/net/OCRe/research.owl#Multiple_human_unit_assignment -->

    <owl:Class rdf:about="&research;Multiple_human_unit_assignment">
        <rdfs:subClassOf rdf:resource="&research;Unit_of_assignment"/>
        <owl:disjointWith rdf:resource="&research;Single_human_unit_assignment"/>
    </owl:Class>
    


    <!-- http://purl.org/net/OCRe/research.owl#Multiple_intervention -->

    <owl:Class rdf:about="&research;Multiple_intervention">
        <rdfs:subClassOf rdf:resource="&research;Intervention_cardinality"/>
        <owl:disjointWith rdf:resource="&research;Single_intervention"/>
    </owl:Class>
    


    <!-- http://purl.org/net/OCRe/research.owl#Multiple_intervention_timing -->

    <owl:Class rdf:about="&research;Multiple_intervention_timing">
        <rdfs:subClassOf rdf:resource="&research;Assignment_characteristics"/>
    </owl:Class>
    


    <!-- http://purl.org/net/OCRe/research.owl#N-of-1_study -->

    <owl:Class rdf:about="&research;N-of-1_study">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&research;Interventional_study"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&research;has_study_characteristic"/>
                        <owl:someValuesFrom rdf:resource="&research;Has_control_group"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&research;has_study_characteristic"/>
                        <owl:someValuesFrom rdf:resource="&research;Within_subject_control_locality"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&research;has_study_characteristic"/>
                        <owl:someValuesFrom>
                            <owl:Class>
                                <owl:unionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="#Data_analyzed_across_one_participant"/>
                                    <rdf:Description rdf:about="#Treatment_assigned_to_one_participant"/>
                                </owl:unionOf>
                            </owl:Class>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
