    <!-- http://purl.org/net/OCRe/study_design.owl#N-of-1_crossover_study -->

    <owl:Class rdf:about="#N-of-1_crossover_study">
        <rdfs:label
            >N-of-1 crossover study</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
