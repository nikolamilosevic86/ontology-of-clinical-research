      <owl:Class rdf:ID="OCRE100012"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="OCRE100076"/>
    </owl:disjointWith>
    <rdfs:subClassOf rdf:resource="#OCRE100072"/>
