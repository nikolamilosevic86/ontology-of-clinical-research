    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/net/OCRe/research.owl#study_characteristic -->

    <owl:ObjectProperty rdf:about="&research;study_characteristic"/>
    


