        <owl:disjointWith rdf:resource="&research;Simultaneous_assignment"/>
    </owl:Class>
    


    <!-- http://purl.org/net/OCRe/research.owl#Simple_randomization -->

    <owl:Class rdf:about="&research;Simple_randomization">
        <rdfs:subClassOf rdf:resource="#Random_allocation"/>
        <statistics:curator>Simona</statistics:curator>
        <statistics:definition
            >Simple randomization is a random allocation scheme that has no restriction, i.e. a single sequence of random assignment. When the sample size is relatively large, simple randomization is expected to produce equal sized treatment groups. [source: http://mahmoodsaghaei.tripod.com/Softwares/randalloc.html ]</statistics:definition>
    </owl:Class>
    


