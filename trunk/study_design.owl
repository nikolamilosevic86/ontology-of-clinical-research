    <!ENTITY Outcome_before_main_predictor "http://purl.org/net/OCRe/study_design.owl#Outcome_before_main_predictor/" >
    <!ENTITY Timing_of_outcome_and_main_predictor "http://purl.org/net/OCRe/study_design.owl#Timing_of_outcome_and_main_predictor/" >
]>


<rdf:RDF xmlns="http://purl.org/net/OCRe/study_design#"
     xml:base="http://purl.org/net/OCRe/study_design"
     xmlns:owl2xml="http://www.w3.org/2006/12/owl2-xml#"
     xmlns:Timing_of_outcome_and_main_predictor="&study_design2;Timing_of_outcome_and_main_predictor/"
     xmlns:study_design2="http://purl.org/net/OCRe/study_design.owl#"
     xmlns:research2="http://purl.org/net/OCRe/research#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:study_design="http://purl.org/net/OCRe/study_design#"
     xmlns:study_protocol="http://purl.org/net/OCRe/study_protocol.owl#"
     xmlns:Outcome_before_main_predictor="&study_design2;Outcome_before_main_predictor/"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:statistics="http://purl.org/net/OCRe/statistics.owl#"
     xmlns:Outcome_after_main_predictor="&study_design2;Outcome_after_main_predictor/"
     xmlns:research="http://purl.org/net/OCRe/research.owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
    <owl:Ontology rdf:about="http://purl.org/net/OCRe/study_design.owl">
        <rdfs:comment rdf:datatype="&xsd;string">An ontology of study design descriptors and categorization of study types in terms of their design descriptors.</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
