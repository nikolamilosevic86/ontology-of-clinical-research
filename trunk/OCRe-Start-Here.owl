    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
  xml:base="http://purl.org/net/OCRe/OCRe-Start-Here">
  <owl:Ontology rdf:about="">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Gathered under "Start-Here" are (1) the core OCRe ontology (research), (2) mapping from OCRe to BFO (bfo-mapping), and (3) examples of studies annotated using OCRe (study_examples).</rdfs:comment>
    <owl:imports rdf:resource="http://purl.org/net/OCRe/study_examples.owl"/>
  </owl:Ontology>
  <owl:Class rdf:about="http://purl.org/net/OCRe/research.owl#Double_blinded">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://purl.org/net/OCRe/research.owl#Blinding_type"/>
    </rdfs:subClassOf>
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Class rdf:about="http://purl.org/net/OCRe/research.owl#Investigator_blinding"/>
          <owl:Class rdf:about="http://purl.org/net/OCRe/research.owl#Subject_blinding"/>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
</rdf:RDF>

