    <!-- http://purl.org/net/OCRe/study_protocol#PlannedEpoch -->

    <owl:Class rdf:about="&study_protocol;PlannedEpoch">
        <rdfs:subClassOf rdf:resource="&study_protocol;Planning_entity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&study_protocol;is_composed_of"/>
