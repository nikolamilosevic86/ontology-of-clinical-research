                </DefinedBy>
                
                <EffectiveTime>
                    <Description>six months</Description>
                </EffectiveTime>
                <EffectiveTime>
                    <Description>one year</Description>
                </EffectiveTime>
                <EffectiveTime>
                    <Description>18 months</Description>
                </EffectiveTime>
                <Name>Continued opiate use as measured by self-report and urinalysis</Name>
                <Priority>secondary</Priority>
            </OutcomeVariable>
            <OutcomeVariable>
                <DefinedBy xsi:type="CDType">
                    
                    <DisplayName>Drug injection behavior - finding</DisplayName>
                    <CodeSystemVersion>2011_01_31</CodeSystemVersion>
                    <CodeSystemName>SNOMED-CT</CodeSystemName>
                    <Code>365984004</Code>	
                    
                </DefinedBy>
                
                <EffectiveTime>
                    <Description>six months</Description>
                </EffectiveTime>
                <EffectiveTime>
                    <Description>one year</Description>
                </EffectiveTime>
                <EffectiveTime>
                    <Description>18 months</Description>
                </EffectiveTime>
                <Name>Self reported frequency of [opiate] injection</Name>
                <Priority>secondary</Priority>
            </OutcomeVariable>
            
            <OutcomeVariable>
                
                <EffectiveTime>
                    <Description>six months</Description>
                </EffectiveTime>
                <EffectiveTime>
                    <Description>one year</Description>
                </EffectiveTime>
                <EffectiveTime>
                    <Description>18 months</Description>
                </EffectiveTime>
                <Name>Self reported frequency of [opiate] injection with previously used injection equipment 
                    (needles, syringes, cookers, cottons, and rinse water)</Name>
                <Priority>secondary</Priority>
            </OutcomeVariable>
            
            <OutcomeVariable>
                <DefinedBy xsi:type="CDType">
                    
                    <DisplayName>High risk sexual behavior</DisplayName>
                    <CodeSystemVersion>2011_01_31</CodeSystemVersion>
                    <CodeSystemName>SNOMED-CT</CodeSystemName>
