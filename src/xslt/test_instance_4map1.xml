        <StudyProtocol xsi:type="InterventionStudyProtocolType">
            <OutcomeVariable>
                <DerivedFrom>
                    <DefinedBy xsi:type="CDType">
                        
                        <DisplayName>Human immunodeficiency virus I infection</DisplayName>
